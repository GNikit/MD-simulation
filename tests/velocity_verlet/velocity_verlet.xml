<?xml version='1.0' encoding='utf-8'?>
<input_variables>
  <setup>
    <dimension>
      <integer_value rank="0">3</integer_value>
    </dimension>
    <steps>
      <integer_value rank="0">5000</integer_value>
    </steps>
    <particles>
      <integer_value rank="1" dim1="dim" shape="3">8 8 8</integer_value>
    </particles>
    <lattice name="SC"/>
    <iterative_method name="VelocityVerlet"/>
    <boundary_conditions name="Periodic"/>
    <rdf_bins>
      <integer_value rank="0">100</integer_value>
    </rdf_bins>
  </setup>
  <io>
    <simulation_name>velocity_verlet_</simulation_name>
    <track_particles name="false"/>
    <energies name="true"/>
    <pressure name="true"/>
    <msd name="true"/>
    <vaf name="true"/>
    <sf name="true"/>
    <rdf name="true"/>
    <positions name="true"/>
  </io>
  <simulation_type name="NormalRun">
    <pair_potential name="GaussianCoreModel">
      <density>
        <real_value rank="0">0.5</real_value>
      </density>
      <temperature>
        <real_value rank="0">1.0</real_value>
      </temperature>
      <dt name="0.005">
        <normalise_with_temperature/>
      </dt>
    </pair_potential>
  </simulation_type>
  <enable_testing/>
</input_variables>
